[remote "origin"]
    url = git@github.com:helm/charts.git
    fetch = +refs/heads/*:refs/remotes/origin/*
[remote "github"]
    url = git@github.com:huangyingw/helm-charts.git
    fetch = +refs/heads/*:refs/remotes/github/*
[push]
    remote = github
[gsync]
    remote = origin
    branch = master
[branch "dev"]
    remote = github
    merge = refs/heads/dev
[branch "master"]
    remote = origin
    merge = refs/heads/master
